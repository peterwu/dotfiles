! General
Xft.dpi:       132
Xft.autohint:  true
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   true
Xft.antialias: true
Xft.rgba:      rgb

! Cursor
!Xcursor.theme:  Bibata_Ice

! XTerm
XTerm*utf8:		 true
XTerm*termName:		 xterm-256color
XTerm*colorBDMode:	 true
XTerm*cursorBlink:	 false
XTerm*scrollBar:	 false
XTerm*selectToClipboard: true
XTerm*omitTranslation: 	 popup-menu
XTerm*saveLines: 	 4096
XTerm*translations: 	 #override \
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
  Ctrl <Key> minus:  smaller-vt-font() \n\
  Ctrl <Key> plus:   larger-vt-font() \n\
  Ctrl <Key> 0:      set-vt-font(d) 

XTerm*faceName:  JetBrains Mono
XTerm*faceSize:  12
XTerm*faceSize1: 6
XTerm*faceSize2: 8
XTerm*faceSize3: 10
XTerm*faceSize4: 14
XTerm*faceSize5: 20
XTerm*faceSize6: 24

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6 
