" let the fun begin
if has('win32')
    let $VIM_HOME = $HOME .. '/vimfiles'
else
    let $VIM_HOME = $HOME .. '/.vim'
endif

source $VIM_HOME/vimrc.d/options.vim
source $VIM_HOME/vimrc.d/keybinds.vim
source $VIM_HOME/vimrc.d/autocmds.vim
source $VIM_HOME/vimrc.d/statusline.vim
source $VIM_HOME/vimrc.d/plugins.vim"
