" let the fun begin
if has('win32')
    let $VIM_HOME = $HOME .. '/vimfiles'
else
    let $VIM_HOME = $HOME .. '/.vim'
endif

source $VIM_HOME/mine/options.vim
source $VIM_HOME/mine/keybinds.vim
source $VIM_HOME/mine/autocmds.vim
source $VIM_HOME/mine/statusline.vim
source $VIM_HOME/mine/plugins.vim"
